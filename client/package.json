{
  "name": "go-debug-bar",
  "version": "0.1.0",
  "description": "Debug bar client for Go web applications - displays requests, queries, and errors in real-time",
  "type": "module",
  "svelte": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "svelte-package -i src/lib -o dist",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "debug",
    "debugbar",
    "go",
    "gin",
    "gorm",
    "svelte",
    "sveltekit"
  ],
  "license": "MIT",
  "devDependencies": {
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "svelte": "^4.0.0",
    "svelte-check": "^3.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0"
  }
}
